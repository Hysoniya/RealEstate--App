{
  "cli": {
    "version": ">= 16.13.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:clean :app:assembleRelease"
      },
      "env": {
        "EXPO_KOTLIN_VERSION": "1.9.10"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
